<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amars.cryptoking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.amars.cryptoking.MainActivity"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.amars.cryptoking.AES"
            android:label="Advanced Encryption"
            android:parentActivityName="com.example.amars.cryptoking.MainActivity"
            />
        <activity android:name="com.example.amars.cryptoking.DES"
            android:label="Data Encryption"
            android:parentActivityName="com.example.amars.cryptoking.MainActivity"/>
        <activity android:name="com.example.amars.cryptoking.RSA"
            android:label="RSA Encryption"
            android:parentActivityName="com.example.amars.cryptoking.MainActivity"/>
        <activity android:name="com.example.amars.cryptoking.MD5"
            android:label="MD5 HASHING"
            android:parentActivityName="com.example.amars.cryptoking.MainActivity"/>
        <activity android:name="com.example.amars.cryptoking.Resetpassword"
            android:label="Set a new password"
            android:parentActivityName="com.example.amars.cryptoking.AES"></activity>
    </application>

</manifest>